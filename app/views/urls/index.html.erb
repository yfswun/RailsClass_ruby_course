<div class="row">
    <div class="twelve columns">
        <h3>Currently Registered URLs</h3>
        <table>
          <tr>
            <th>Course</th>
            <th>Student ID</th>
            <th>URL</th>
          </tr>
            <% @students.each do |s| %>
              <% s.urls.each do |u| %>
                <tr>
                  <td><%= s.course %></td>
                  <td><%= s.name %></td>
                  <td><a href="<%= u.url %>" title="<%= s.name %>"><%= u.url %></a></td>
                </tr>
              <% end %>
            <% end %>
        </table>
    </div>
    <%= link_to 'Enter another URL', '/',:html => {:class => 'nav'} %>
</div>
