<div class="row">
    <div class="twelve columns" style="background-color:#f0f0f0;">
        <h1>Listing Students</h1>
        <table>
            <tr>
                <th>Student ID</th>
                <th>Course</th>
                <th>URL</th>
            </tr>
            <% @students.each do |student| %>
              <tr>
                <td><%= student.name %></td>
                <td><%= student.course %></td>
                <td><% student.urls.each do |u| %>
                      <a href="<%= u.url %>"><%= u.url %></a>
                    <% end %>
                </td>
              </tr>
            <% end %>
        </table>
        <div style="margin:1em 0; height:10px;clear:both;"><br /></div>
        <p><%= link_to 'New Student', new_student_path %></p>
    </div>
</div>
